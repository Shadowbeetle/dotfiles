{
  "coc.preferences.formatOnSaveFiletypes": [
    "css", 
    "javascript", 
    "javascriptreact", 
    "typescript",
    "typescriptreact",
    "markdown"
  ],
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "javascript", "javascriptreact", "typescriptreact", "graphql"],
      "trace.server": "verbose"
    }
  }
}

